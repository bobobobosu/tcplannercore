            <undoMoveTabuSize>5</undoMoveTabuSize>
        </acceptor>
        <forager>
            <pickEarlyType>FIRST_BEST_SCORE_IMPROVING</pickEarlyType>
            <acceptedCountLimit>350</acceptedCountLimit>
            <finalistPodiumType>STRATEGIC_OSCILLATION_BY_LEVEL</finalistPodiumType>
        </forager>
    </localSearch>
</solver>