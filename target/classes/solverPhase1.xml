<solver>
    <environmentMode>REPRODUCIBLE</environmentMode>
    <moveThreadCount>AUTO</moveThreadCount>
    <solutionClass>bo.tc.tcplanner.domain.Schedule</solutionClass>
    <entityClass>bo.tc.tcplanner.domain.Allocation</entityClass>
    <scoreDirectorFactory>
        <scoreDrl>TCRules_P1.drl</scoreDrl>
    </scoreDirectorFactory>
    <termination>
        <!--        <millisecondsSpentLimit>100</millisecondsSpentLimit>-->
        <bestScoreLimit>[0/0/0/0/0]hard/[-2147483648/-2147483648/-2147483648/-2147483648]soft</bestScoreLimit>
    </termination>
    <localSearch>
        <unionMoveSelector>
            <moveListFactory>
                <fixedProbabilityWeight>0.1</fixedProbabilityWeight>
                <moveListFactoryClass>bo.tc.tcplanner.domain.solver.moves.SkippedSwapMoveFactory</moveListFactoryClass>
            </moveListFactory>
            <changeMoveSelector>
                <fixedProbabilityWeight>0.0414</fixedProbabilityWeight>
                <entitySelector>
                    <cacheType>PHASE</cacheType>
                    <selectionOrder>PROBABILISTIC</selectionOrder>
                    <filterClass>bo.tc.tcplanner.domain.solver.filters.IsFocusedFilter</filterClass>
                    <filterClass>bo.tc.tcplanner.domain.solver.filters.ProgressDeltaCanChangeFilter</filterClass>
                    <probabilityWeightFactoryClass>
                        bo.tc.tcplanner.domain.solver.meters.AllocationProbabilityWeightFactory
                    </probabilityWeightFactoryClass>
                </entitySelector>
                <valueSelector variableName="progressdelta"/>
            </changeMoveSelector>
            <changeMoveSelector>
                <fixedProbabilityWeight>0.0414</fixedProbabilityWeight>
                <entitySelector>
                    <filterClass>bo.tc.tcplanner.domain.solver.filters.TimelineEntryCanChangeFilter</filterClass>
                </entitySelector>
                <valueSelector variableName="timelineEntry"/>
            </changeMoveSelector>
            <changeMoveSelector>
                <fixedProbabilityWeight>0.0036</fixedProbabilityWeight>
                <entitySelector>
                    <cacheType>PHASE</cacheType>
                    <selectionOrder>PROBABILISTIC</selectionOrder>
                    <filterClass>bo.tc.tcplanner.domain.solver.filters.IsFocusedFilter</filterClass>
                    <filterClass>bo.tc.tcplanner.domain.solver.filters.DelayCanChangeFilter</filterClass>
                    <probabilityWeightFactoryClass>
                        bo.tc.tcplanner.domain.solver.meters.AllocationProbabilityWeightFactory
                    </probabilityWeightFactoryClass>
                </entitySelector>
                <valueSelector variableName="delay"/>
            </changeMoveSelector>
            <cartesianProductMoveSelector>
                <fixedProbabilityWeight>0.59616</fixedProbabilityWeight>
                <changeMoveSelector>
                    <entitySelector id="entitySelector">
                        <cacheType>PHASE</cacheType>
                        <selectionOrder>PROBABILISTIC</selectionOrder>
                        <filterClass>bo.tc.tcplanner.domain.solver.filters.TimelineEntryCanChangeFilter</filterClass>
                        <probabilityWeightFactoryClass>
                            bo.tc.tcplanner.domain.solver.meters.AllocationProbabilityWeightFactory
                        </probabilityWeightFactoryClass>
                    </entitySelector>
                    <valueSelector variableName="timelineEntry"/>
                </changeMoveSelector>
                <changeMoveSelector>
                    <entitySelector mimicSelectorRef="entitySelector"/>
                    <valueSelector variableName="progressdelta"/>
                </changeMoveSelector>
                <ignoreEmptyChildIterators>true</ignoreEmptyChildIterators>
            </cartesianProductMoveSelector>
            <cartesianProductMoveSelector>
                <fixedProbabilityWeight>0.05184</fixedProbabilityWeight>
                <changeMoveSelector>
                    <entitySelector>
                        <cacheType>PHASE</cacheType>
                        <selectionOrder>PROBABILISTIC</selectionOrder>
                        <filterClass>bo.tc.tcplanner.domain.solver.filters.IsFocusedFilter</filterClass>
                        <filterClass>bo.tc.tcplanner.domain.solver.filters.DelayCanChangeFilter</filterClass>
                        <probabilityWeightFactoryClass>
                            bo.tc.tcplanner.domain.solver.meters.AllocationProbabilityWeightFactory
                        </probabilityWeightFactoryClass>
                    </entitySelector>
                    <valueSelector variableName="delay"/>
                </changeMoveSelector>
                <changeMoveSelector>
                    <entitySelector>
                        <filterClass>bo.tc.tcplanner.domain.solver.filters.IsFocusedFilter</filterClass>
                        <filterClass>bo.tc.tcplanner.domain.solver.filters.DelayCanChangeFilter</filterClass>
                    </entitySelector>
                    <valueSelector variableName="delay"/>
                </changeMoveSelector>
                <ignoreEmptyChildIterators>true</ignoreEmptyChildIterators>
            </cartesianProductMoveSelector>
        </unionMoveSelector>
        <acceptor>
            <entityTabuRatio>0.02</entityTabuRatio>
            <moveTabuSize>1</moveTabuSize>
            <undoMoveTabuSize>5</undoMoveTabuSize>
            <lateAcceptanceSize>1</lateAcceptanceSize>
        </acceptor>
        <forager>
            <acceptedCountLimit>4</acceptedCountLimit>
            <finalistPodiumType>STRATEGIC_OSCILLATION_BY_LEVEL</finalistPodiumType>
        </forager>
    </localSearch>
</solver>